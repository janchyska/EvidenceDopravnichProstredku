package prostredky;

/**
 *
 * @author janch
 */
public enum Barva {
    CERNA("černá"),
    SEDA("šedá"),
    BILA("bílá"),
    CERVENA("červená"),
    MODRA("modrá"),
    JINA("jiná");


    private final String barva;

    private Barva(String barva) {
        this.barva = barva;
    }

    public String barva() {
        return this.barva;
    }

    public String getBarva() {
        return this.barva;
    }
    
    @Override
    public String toString() {
        return this.barva;
    }

}
